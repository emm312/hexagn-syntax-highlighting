{
	"name": "hexagn",
	"repository": "https://github.com/emm312/hexagn-syntax-highlighting",
	"displayName": "Hexagn",
	"description": "extension for hexagn, a language compiled to URCL",
	"version": "2.0.0",
	"publisher": "emm312",
	"engines": {
		"vscode": "^1.70.0"
	},
	"categories": [
		"Programming Languages"
	],
	"activationEvents": [
		"onLanguage:hexagn"
	],
	"main": "./out/extension.js",
	"contributes": {
		"languages": [
			{
				"id": "hexagn",
				"aliases": [
					"Hexagn",
					"hexagn"
				],
				"extensions": [
					"hxgn"
				],
				"configuration": "./language-configuration.json"
			}
		],
		"grammars": [
			{
				"language": "hexagn",
				"scopeName": "source.hxgn",
				"path": "./syntaxes/hexagn.tmLanguage.json"
			}
		],
		"snippets": [
			{
				"language": "hexagn",
				"path": "./snippets/hexagn.json"
			}
		]
	},
	"__metadata": {
		"id": "9b929f93-1316-40c9-b8ab-725bc69e851c",
		"publisherDisplayName": "emm312",
		"publisherId": "aa79138a-84dc-4afa-946e-5d9edc26904f",
		"isPreReleaseVersion": false
	}
}
